{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "properties": {
    "metaInfo": {
      "type": "object",
      "required": true,
      "properties": {
        "wType": {
          "type": "string",
          "required": true
        },
        "version": {
          "type": "string",
          "required": true
        },
        "heading": {
          "type": "string",
          "required": true
        },
        "description": {
          "type": "string",
          "required": true
        },
        "frequency": {
          "type": "number",
          "required": true
        }
      }
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "required": true
          },
          "val": {
            "type": "number",
            "required": true
          }
        }
      },
      "required": true
    },
    "link": {
      "type": "object",
      "required": true,
      "properties": {
        "url": {
          "type": "string",
          "required": true
        },
        "active": {
          "type": "boolean",
          "required": true
        }
      }
    },
    "health": {
      "type": "object",
      "required": true,
      "properties": {
        "status": {
          "type": "string",
          "required": true
        },
        "color": {
          "type": "string",
          "required": true
        },
        "active": {
          "type": "boolean",
          "required": true
        }
      }
    }
  }
}